{
  "name": "my-local-knowledge",
  "version": "1.0.0",
  "type": "module",
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/js-yaml": "^4.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.29",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-vue": "^10.1.0",
    "express": "^5.1.0",
    "mermaid": "^11.6.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "sass": "^1.89.1",
    "scss": "^0.2.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "uuid": "^11.1.0",
    "vitepress": "^1.6.3",
    "vitepress-plugin-mermaid": "^2.0.17",
    "vue-eslint-parser": "^10.1.3"
  },
  "scripts": {
    "docs:dev": "vitepress dev . --port 4865",
    "docs:devo": "vitepress dev . --port 4865 --open",
    "docs:build": "vitepress build .",
    "docs:preview": "vitepress preview .",
    "lint": "eslint . --ext .vue,.js,.ts,.mts",
    "lint:fix": "eslint . --ext .vue,.js,.ts,.mts --fix",
    "format": "prettier --write .",
    "dev:backend": "chcp 65001 && nodemon --watch server --ext ts --exec tsx server/index.ts",
    "dev": "concurrently -n \"FRONTEND,BACKEND\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run docs:devo\" \"npm run dev:backend\""
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "element-plus": "^2.9.11",
    "js-yaml": "^4.1.0",
    "md-editor-v3": "^5.6.0",
    "multer": "^2.0.1",
    "vue": "^3.5.16"
  }
}
